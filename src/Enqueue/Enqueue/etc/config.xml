<?xml version="1.0" encoding="UTF-8"?>
<config>
    <modules>
        <Enqueue_Enqueue>
            <version>0.1</version>
        </Enqueue_Enqueue>
    </modules>
    <global>
        <models>
            <enqueue>
                <class>Enqueue_Enqueue_Model</class>
            </enqueue>
        </models>
        <helpers>
            <enqueue>
                <class>Enqueue_Enqueue_Helper</class>
            </enqueue>
        </helpers>
    </global>
    <default>
        <enqueue>
            <transport>
                <default>rabbitmq-amqp</default>
            </transport>
            <client>
                <prefix>enqueue</prefix>
                <app_name>app</app_name>
                <router_topic>router</router_topic>
                <router_queue>default</router_queue>
                <default_processor_queue>default</default_processor_queue>
                <redelivered_delay_time>0</redelivered_delay_time>
            </client>
            <rabbitmq_amqp>
                <host>localhost</host>
                <port>5672</port>
                <login>guest</login>
                <password>guest</password>
                <vhost>/</vhost>
                <lazy>1</lazy>
                <delay_plugin_installed>0</delay_plugin_installed>
            </rabbitmq_amqp>
            <amqp>
                <port>5672</port>
                <lazy>1</lazy>
            </amqp>
            <rabbitmq_stomp>
                <management_plugin_port>15672</management_plugin_port>
                <port>61613</port>
                <lazy>1</lazy>
            </rabbitmq_stomp>
            <stomp>
                <port>61613</port>
                <lazy>1</lazy>
            </stomp>
            <fs>
                <pre_fetch_count>1</pre_fetch_count>
                <chmod>0600</chmod>
            </fs>
            <sqs>
                <retries>3</retries>
                <lazy>1</lazy>
            </sqs>
            <redis>
                <port>6379</port>
                <lazy>1</lazy>
            </redis>
            <dbal>
                <table_name>enqueue</table_name>
                <polling_interval>1000</polling_interval>
                <lazy>1</lazy>
            </dbal>
        </enqueue>
    </default>
</config>
